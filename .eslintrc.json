{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "import/no-dynamic-require": "off",
        "global-require": "off",
        "camelcase": "off",
        "no-await-in-loop": "off",
        "no-unused-vars": "off",
        "no-plusplus": "off",
        "no-restricted-syntax": "off",
        "radix": "off",
        "consistent-return": "off",
        "no-inner-declarations": "off"
    }
}
